{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc --build --clean && tsc && tsc-alias",
    "serve": "node dist/index.js",
    "pre-deploy": "prisma generate",
    "migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "db:push": "prisma db push --schema prisma/schema.prisma",
    "db:studio": "prisma studio --schema prisma/schema.prisma",
    "db:seed": "ts-node prisma/seed.ts",
    "install:all": "yarn install",
    "clean": "rimraf dist node_modules yarn.lock",
    "reset": "yarn clean && yarn install",
    "test": "echo \"No tests specified\" && exit 0",
    "test:api": "ts-node -e \"require('./test-api').main()\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.8.2",
    "redis": "^4.6.10",
    "ts-node": "^10.9.2",
    "uuidv7": "^1.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.14",
    "@types/nodemailer": "^6.4.17",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.8.3"
  }
}
